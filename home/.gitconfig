[user]
	email = magnusboye@gmail.com
	name = Magnus Boye
[core]
    editor = vim
    excludesfile = ~/.gitignore
[pull]
    rebase = true
[rerere]
    enabled = true
[log]
    decorate = true
[alias]
    tree = log --decorate --graph --oneline
    lone = log --oneline --no-decorate
    mod = log -1 --name-status
    stack = cherry -v
    ds = diff --staged
    ca = commit -a --amend --no-edit
[diff]
    wsErrorHighlight = all
